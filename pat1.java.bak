import java.util.Scanner;
class pat1
{
	public static void main(String[] args) 
	{
		Scanner scn=new Scanner(System.in);
		System.out.print("Enter the num:");
		int num=scn.nextInt();
		
		for (int i=num;i>0;i--)
		{
			for (int j=i;j>0;j--)
			{
				if(j==1){
					System.out.print(j+"*");
				}
				else{
					System.out.print(j+" ");
				}
			}System.out.println();			
		}
		
	}
}
